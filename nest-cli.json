{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": ".",
  "entryFile": "src/main.js",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "prisma/**/*"
      },
      {
        "include": "package.json"
      },
      {
        "include": "pnpm-lock.yaml"
      },
      {
        "include": "assets/**/*"
      },
      {
        "include": "templates/**/*"
      }
    ]
  }
}
